<h1>Edit User</h1>
<h3>User <%= @user.id %></h3>
<p><%= link_to "Back to Users", users_path %></p>

<%= form_with model: @user, local: true do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>

  <%= f.label :email %>
  <%= f.email_field :email %><br>

  <%= f.submit "Edit User" %>
<% end %>
