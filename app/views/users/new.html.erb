<h1>New User</h1>

<%= form_with model: @user, local: true do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>

  <%= f.label :email %>
  <%= f.email_field :email %><br>

  <%= f.submit "Create User" %>
<% end %>
